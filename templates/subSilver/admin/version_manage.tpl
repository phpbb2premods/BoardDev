<h1>{L_VERSION_MANAGER}</h1>
<p>{L_VERSION_MANAGER_EXPLAIN}</p>
<form action="{S_VERSION_ACTION}" method="post">
<table width="99%" cellpadding="4" cellspacing="1" border="0" align="center" class="forumline">
	<tr>
	  <th class="thHead" colspan="2">{L_VERSION_MANAGER}</th>
	</tr>
	<!-- BEGIN version_manage_loop -->
	<tr>
		<td class="{version_manage_loop.ROW_CLASS}">{version_manage_loop.L_MOD_NAME}</td>
		<td class="{version_manage_loop.ROW_CLASS}"><input type="radio" name="{version_manage_loop.S_MOD_ID}" value="1" {version_manage_loop.S_MOD_ENABLED} />&nbsp;{L_ENABLE}&nbsp;&nbsp;<input type="radio" name="{version_manage_loop.S_MOD_ID}" value="0" {version_manage_loop.S_MOD_DISABLED} />&nbsp;{L_DISABLE}</td>
	</tr>
	<!-- END version_manage_loop -->
	<tr>
        <td class="catBottom" colspan="2" align="center">{S_HIDDEN_FIELDS}<input type="submit" name="submit" value="{L_SUBMIT}" class="mainoption" /></td>
    </tr>
</table>
</form>
<br />